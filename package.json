{
  "name": "search",
  "version": "1.0.0",
  "description": "youtube search video app",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "serve": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run serve\" \"npm run client\"",
    "cluster": "node cluster.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiCkEyZzZ/search.git"
  },
  "keywords": [
    "mongodb",
    "express",
    "react",
    "node"
  ],
  "author": "Mikhail Zhuravlev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MiCkEyZzZ/search/issues"
  },
  "homepage": "https://github.com/MiCkEyZzZ/search#readme",
  "dependencies": {
    "autocannon": "^6.5.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.23",
    "nodemailer": "^6.4.11",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^6.0.3",
    "nodemon": "^2.0.4"
  }
